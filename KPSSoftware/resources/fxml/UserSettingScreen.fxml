                    <image>
                        <Image url="@../img/KPS.png"/>
                    </image>
                </ImageView>
                <Button id="Exit" layoutX="487.0" layoutY="516.0" mnemonicParsing="false" onAction="#changeScenes"
                        prefHeight="60.0" prefWidth="75.0" styleClass="discard-button" stylesheets="@../css/style.css"
